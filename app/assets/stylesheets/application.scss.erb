body {
  width: 100%;
  height: 100%;
  font-family: ProximaRegular,sans-serif;
  font-weight: 300;
  color: #666;
  background-color: #fff;
}

html {
  width: 100%;
  height: 100%;
}

p {
  margin: 0 0 20px;
  font-size: 16px;
  line-height: 1.6em;
}

.slide  {
  min-height: 100vh;
}

h1, h2, h3, h4, h5, h6 {
  margin: 0 0 30px;
  text-transform: uppercase;
  font-family: ProximaRegular,sans-serif;
  font-weight: 700;
  letter-spacing: 1px;
}

