blockquote {
    // border-top: 1px solid #ccc;
    // border-bottom: 1px solid #ccc;
    border-left: 0px;
// font-family: Georgia, serif;
font-family: ProximaRegular,sans-serif;
font-size: 30px;
font-style: italic;
text-align: center;

  margin: 30px;
  padding: 30px;
line-height: 1.45;
position: relative;
color: #383838;
}

blockquote:before {
display: block;
content: "\201C";
font-size: 80px;
position: absolute;
left: -20px;
top: -20px;
color: #7a7a7a;
}

blockquote cite {
color: #999999;
font-size: 20px;
display: block;
margin-top: 5px;
text-align: right;
}

blockquote cite:before {
content: "\2014 \2009";
}

